	$('#certificate-form').submit(function(e){
		e.preventDefault();
		var formData = $('#certificate-form').serialize();
		
		$.ajax({
			type:'POST',
			url:site_url+'check-certificate',
			data:formData,
			dataType:'JSON',
			beforeSend:function(){
	            $('#certificate-form').find('.msg').html("Please Wailt..Processing..").show();
	        },
			success:function(data){
				if(data.status==0){
					$('#certificate-form').find('.msg').html(data.msg);
				}else if(data.status==1){
					window.location = site_url+'verify-certificate/y/'+data.certi_id;
				}else if(data.status==2){
					window.location = site_url+'verify-certificate/n/'+data.certi_id;
				}else{
					$('#certificate-form').find('.msg').html('Something Went Wrong.');
				}
			}
		});
	}) ;
	$(document).ready(function(){
		//course inquiry
		$.validate({

	    	form : '#inquiry-form',
		    onSuccess : function(form) {
		        $.ajax({
		            url: site_url+'contact/course_inquiry_ajax',
		            type: "post",
		            data: $(form).serialize(),
		            beforeSend:function(){
			            $('#inquiry-form-err').html("Please Wait...Processing...");
			            $('#inq-btn').attr("disabled",true);
			        },
			        success: function (response) {
			            $('#inq-btn').attr("disabled",false);
			        	if(response.trim()=="success"){
			        		$('#inquiry').modal('hide');
				            $('#inquiry-form-err').html("");
				        	$('#thank-you-modal').modal('show');
				        	$('#thank-you-msg').html(response);
			        	}else{
			            	$('#inquiry-form-err').html(response);

			        	}
			        	
			        }, error: function (jqXHR, exception) {
			            $('#inq-btn').attr("disabled",false);
				        var msg = '';
				        if (jqXHR.status === 0) {
				            msg = 'Not connect.\n Verify Network.';
				        } else if (jqXHR.status == 404) {
				            msg = 'Requested page not found. [404]';
				        } else if (jqXHR.status == 500) {
				            msg = 'Internal Server Error [500].';
				        } else if (exception === 'parsererror') {
				            msg = 'Requested JSON parse failed.';
				        } else if (exception === 'timeout') {
				            msg = 'Time out error.';
				        } else if (exception === 'abort') {
				            msg = 'Ajax request aborted.';
				        } else {
				            msg = 'Uncaught Error.\n' + jqXHR.responseText;
				        }
				        $('#inquiry-form-err').html(msg);
				    },           
		        });
		        return false;
		    }
		});
