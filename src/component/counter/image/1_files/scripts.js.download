	//password show/hide
	$('.showOrHide').click(function(e){
	    var target = e.currentTarget
	    if($(target).hasClass('show')){
	    	$(target).removeClass('show').addClass('hide')
    		$(target).parent('div').prev('input').attr('type','password')
    		$(target).find('i').addClass('fa-eye').removeClass('fa-eye-slash');
	    }else{
	    	$(target).removeClass('hide').addClass('show')
    		$(target).parent('div').prev('input').attr('type','text')
    		$(target).find('i').addClass('fa-eye-slash').removeClass('fa-eye');

	    }
	})
