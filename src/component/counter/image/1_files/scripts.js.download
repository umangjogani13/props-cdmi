$('#student-img').on('change', function () { 
	var name = document.getElementById("student-img").files[0].name;
  	var ext = name.split('.').pop().toLowerCase();
  	if(jQuery.inArray(ext, ['gif','png','jpg','jpeg']) == -1) 
  	{
   		alert("Invalid Image File");
   		return false;
  	}
  	var reader = new FileReader();
	$('#profile-pic-crop').modal('show');
    
    reader.onload = function (e) {
        $uploadCrop.croppie('bind', {
            url: e.target.result
        }).then(function(){
            console.log('jQuery bind complete');
        });
            
    }
    reader.readAsDataURL(this.files[0]);
});
     
$('.save-image').on('click', function (ev) {
    var $that = $(this);
    $uploadCrop.croppie('result', {
        type: 'canvas',
        size: 'viewport'
    }).then(function (resp) {
        $.ajax({
            url: site_url+"Student_Dashboard/student_img",
            type: "POST",
            data: {"image":resp},
            beforeSend:function(){
                $('#save-img-msg').html("Please Wait...").show();
                $that.attr("disabled",true);
            },
            success: function (data) {
                $('#save-img-msg').html("").hide();
                $that.attr("disabled",false);
                if(data.trim()=="success"){
                	$('#user-img').attr('src',resp);
					$('#profile-pic-crop').modal('hide');
					$('#thank-you-modal12').modal('show');
                }else{
                	$('#save-img-msg').html(data).show();
                }
            }
        });
    });
});
